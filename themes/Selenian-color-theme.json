{
  "name": "Selenian",
  "type": "dark",
  "colors": {
    "editor.background": "#141414",
    "editor.foreground": "#d8d8d8",
    "editor.selectionBackground": "#323232",
    "activityBarBadge.background": "#4e4eff",
    "sideBarTitle.foreground": "#d8d8d8",
    "activityBar.background": "#282828",
    "statusBar.background": "#282828",
    "editorCursor.foreground": "#ffc7b2"
  },
  "tokenColors": [
    {
      "name": "Type",
      "scope": "entity.name.type",
      "settings": {
        "foreground": "#e1d3bb"
      }
    },
    {
      "name": "Comment",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#e0bcbc"
      }
    },
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.parameter",
        "string constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Colors",
      "scope": ["constant.other.color"],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#c3a6d6"
      }
    },
    {
      "name": "Keyword Control",
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#f9f9f9"
      }
    },
    {
      "name": "Function, Special Method",
      "scope": [
        "entity.name.function",
        "meta.function-call",
        "variable.function",
        "support.function",
        "keyword.other.special-method",
        "entity.name.method.js",
        "meta.class-method.js entity.name.function.js",
        "variable.function.constructor"
      ],
      "settings": {
        "foreground": "#adaddb"
      }
    },
    {
      "name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
      "scope": [
        "constant.numeric",
        "constant.language",
        "support.constant",
        "constant.character",
        "constant.escape",
        "keyword.other.unit",
        "source.js constant.other.object.key.js string.unquoted.label.js"
      ],
      "settings": {
        "foreground": "#e4c2e4"
      }
    },
    {
      "name": "String, Symbols, Inherited Class, Markup Heading",
      "scope": ["string"],
      "settings": {
        "foreground": "#bfe7e7"
      }
    },
    {
      "name": "Class, Support",
      "scope": [
        "support.type",
        "support.class",
        "support.orther.namespace.use.php",
        "meta.use.php",
        "support.other.namespace.php",
        "markup.changed.git_gutter",
        "support.type.sys-types",
        "tag.decorator.js entity.name.tag.js",
        "tag.decorator.js punctuation.definition.tag.js"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "CSS Class and Support",
      "scope": [
        "source.css support.type.property-name",
        "source.sass support.type.property-name",
        "source.scss support.type.property-name",
        "source.less support.type.property-name",
        "source.stylus support.type.property-name",
        "source.postcss support.type.property-name",
        "support.type"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Sub-methods",
      "scope": [
        "entity.name.module.js",
        "variable.import.parameter.js",
        "variable.other.class.js"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "HTML Attributes",
      "scope": [
        "text.html.basic entity.other.attribute-name.html",
        "text.html.basic entity.other.attribute-name"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "CSS Classes",
      "scope": ["entity.other.attribute-name.class"],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "CSS ID's",
      "scope": [
        "source.sass keyword.control",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#d8d8d8"
      }
    },
    {
      "name": "Inserted",
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#7dae4e"
      }
    },
    {
      "name": "Deleted",
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#ca8893"
      }
    },
    {
      "name": "Changed",
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#c3a6d6"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "Escape Characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#89DDFF"
      }
    },
    {
      "name": "URL",
      "scope": ["*url*", "*link*", "*uri*"],
      "settings": {
        "fontStyle": "underline"
      }
    }
  ]
}
